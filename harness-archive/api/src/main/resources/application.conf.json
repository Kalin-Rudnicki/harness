{
  "telemetry": {
    "sources": {
      "logged": {
        "logTolerance": "TRACE"
      }
    }
  },
  "logging": {
    "context": {},
    "sources": {
      "std-out": {
        "logTolerance": "INFO",
        "colorMode": "EXTENDED"
      }
    }
  },
  "db": {
    "target": {
      "database": "archive",
      "host": "localhost",
      "port": "5000"
    },
    "credentials": {
      "username": "postgres"
    },
    "pool": {
      "minConnections": 4,
      "maxConnections": 16,
      "duration": "P60S"
    }
  },
  "http": {
    "port": 3000,
    "resDir": "harness-archive/res",
    "useJarResource": false
  },
  "email": {
    "client": {
      "host": "smtp.gmail.com",
      "port": 465,
      "authType": "SSL"
    },
    "service": {
      "sender": "harness.email.client@gmail.com"
    }
  }
}
