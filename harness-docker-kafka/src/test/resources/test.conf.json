{
  "logging": {
    "context": {},
    "sources": {
      "std-out": {
        "logTolerance": "TRACE",
        "colorMode": "EXTENDED"
      }
    }
  },
  "telemetry": {
    "sources": {
      "logged": {
        "logTolerance": "TRACE"
      }
    }
  },
  "docker": {
    "needsSudo": true,
    "appName": "kafka-test-main",
    "kafka": {
      "zookeeper": {
        "allowAnonymousLogin": true
      },
      "broker": {
        "allowPlaintextListener": true,
        "brokerId": 1
      }
    }
  },
  "kafka": {
    "zookeeper": {
      "port": 6001
    },
    "broker": {
      "plainTextPort": 6101,
      "plainTextHostPort": 6201
    },
    "consumer": {
      "groupId": "my-group",
      "topicName": "my-topic"
    },
    "producer": {
      "topicName": "my-topic"
    }
  }
}