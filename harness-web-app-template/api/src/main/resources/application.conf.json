{
  "logging": {
    "context": {},
    "sources": {
      "std-out": {
        "logTolerance": "DETAILED",
        "colorMode": "EXTENDED"
      }
    }
  },
  "telemetry": {
    "sources": {
      "logged": {
        "logTolerance": "TRACE"
      }
    }
  },
  "db": {
    "target": {
      "database": "template",
      "host": "localhost",
      "port": 5001
    },
    "credentials": {
      "username": "postgres"
    },
    "pool": {
      "minConnections": 4,
      "maxConnections": 16,
      "duration": "P60S"
    }
  },
  "http": {
    "port": 3001,
    "resDir": "harness-web-app-template/res",
    "useJarResource": false,
    "ssl": {
      "certificateRef": "ssl/localhost.pem",
      "certificateRefType": "Jar",
      "privateKeyRef": "ssl/localhost-key.pem",
      "privateKeyRefType": "Jar"
    }
  },
  "email": {
    "client": {
      "host": "smtp.gmail.com",
      "port": 465,
      "authType": "SSL"
    },
    "service": {
      "sender": "harness.email.client@gmail.com"
    }
  },
  "ui": {
    "logTolerance": "DETAILED"
  }
}